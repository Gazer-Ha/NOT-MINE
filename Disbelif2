{Name = "Frolic", ID = 119460860418071},
{Name = "Funeral For The Living", ID = 123297701965318},
{Name = "Flexing", ID = 80121910804727},
}

local idle, walk, run = nil, nil, nil

-- UI
local gui = Instance.new("ScreenGui", cloneref(game:GetService("CoreGui")))
gui.Name = "AnimUI"
gui.ResetOnSpawn = false

local f = Instance.new("Frame", gui)
f.Size = UDim2.new(0, 240, 0, 200)
f.Position = UDim2.new(0.5, -120, 0.5, -100)
f.BackgroundColor3 = Color3.fromRGB(25, 25, 25)
f.Active = true
f.Draggable = true
Instance.new("UICorner", f).CornerRadius = UDim.new(0, 6)

local sf = Instance.new("ScrollingFrame", f)
sf.Size = UDim2.new(1, 0, 1, 0)
sf.CanvasSize = UDim2.new(0, 0, 0, 0)
sf.ScrollBarThickness = 4
sf.BackgroundTransparency = 1
sf.AutomaticCanvasSize = Enum.AutomaticSize.Y
sf.BorderSizePixel = 0

local layout = Instance.new("UIListLayout", sf)
layout.Padding = UDim.new(0, 2)

local btns = {}

local function updateIndicators()
	for _, data in ipairs(btns) do
		local btn, typ = data.Button, data.Type
		local sel = (typ == "Idle" and idle == data.ID) or
		            (typ == "Walk" and walk == data.ID) or
		            (typ == "Run" and run == data.ID)
		btn.BackgroundColor3 = sel and Color3.fromRGB(60, 130, 60) or Color3.fromRGB(40, 40, 40)
	end
	if idle and walk and run then
		gui:Destroy()
		print("Idle:", idle, "Walk:", walk, "Run:", run)
		IdleId = idle
		WalkId = walk
		RunId = run
	end
end

for _, v in ipairs(Anims5) do
	local btn = Instance.new("TextButton")
	btn.Size = UDim2.new(1, -8, 0, 24)
	btn.Position = UDim2.new(0, 4, 0, 0)
	btn.BackgroundColor3 = Color3.fromRGB(40, 40, 40)
	btn.TextColor3 = Color3.new(1, 1, 1)
	btn.Font = Enum.Font.SourceSans
	btn.TextSize = 15
	btn.Text = v.Name
	btn.BorderSizePixel = 0
	btn.Parent = sf

	local btnType
	local nameLower = v.Name:lower()
	if nameLower:find("idle") then
		btnType = "Idle"
	elseif nameLower:find("walk") then
		btnType = "Walk"
	elseif nameLower:find("run") then
		btnType = "Run"
	end

	btn.MouseButton1Click:Connect(function()
		if btnType == "Idle" then idle = v.ID
		elseif btnType == "Walk" then walk = v.ID
		elseif btnType == "Run" then run = v.ID end
		updateIndicators()
	end)

	table.insert(btns, {Button = btn, Type = btnType, ID = v.ID})
end


for i, v in pairs(InkGame6) do
if v.Divider then
DancesE:Line()
else
DancesE:Button(v.Name, v.Name, function()
if not Anims:FindFirstChild(v.Name) then
CreateAnimation(v.Name, v.ID)
end
PlayAnim(Character, Anims[v.Name], 0.1, (v.Speed or 1))
end)
end
end

