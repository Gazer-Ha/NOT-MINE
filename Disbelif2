CreateAnimation("Jump", 84397577392351)
CreateAnimation("Fall", 90270295182351)
game:GetService("CoreGui").FluxLib.MainFrame.Visible = false
repeat task.wait() until IdleId and WalkId and RunId and IdleId ~= "" and WalkId ~= "" and RunId ~= ""
game:GetService("CoreGui").FluxLib.MainFrame.Visible = true
print("Succes")
CreateAnimation("Idle", IdleId)
CreateAnimation("Walk", WalkId)
CreateAnimation("Run", RunId)


game:GetService("RunService").Heartbeat:Connect(function()
	for _, track in pairs(Character:FindFirstChildOfClass("Humanoid"):GetPlayingAnimationTracks()) do
		track:Stop(0)
	end

	local moveThreshold = 0.01
	local runThreshold = 15
	local verticalThreshold = 0.5

	local HitFloor, HitPosition = Raycast(
		RootPart.Position,
		(CFrame.new(RootPart.Position, RootPart.Position + Vector3.new(0, -1, 0))).LookVector,
		4,
		Character
	)

	local velocity = RootPart.Velocity
	local TorsoVelocity = velocity.Magnitude
	local TorsoVerticalVelocity = velocity.Y

	if not HitFloor and math.abs(TorsoVerticalVelocity) > verticalThreshold then
		-- Always stop ground animations when airborne
		StopAnim(Character, Anims.Walk)
		StopAnim(Character, Anims.Run)

		if TorsoVerticalVelocity > 0 then
			if not Jumped then
				StopAnim(Character, Anims.Fall)
				StopAnim(Character, Anims.Idle)

				PlayAnim(Character, Anims.Jump, 0.05, 1)
				Jumped = true
				Falled, Idled, Walked, Ran = false, false, false, false
			end
		else
			if not Falled then
				StopAnim(Character, Anims.Jump)
				StopAnim(Character, Anims.Idle)

				PlayAnim(Character, Anims.Fall, 0.05, 1)
				Falled = true
				Jumped, Idled, Walked, Ran = false, false, false, false
			end
		end

	elseif HitFloor and math.abs(TorsoVerticalVelocity) <= verticalThreshold then
		if TorsoVelocity < moveThreshold then
			if not Idled then
				StopAnim(Character, Anims.Jump)
				StopAnim(Character, Anims.Fall)
				StopAnim(Character, Anims.Walk)
				StopAnim(Character, Anims.Run)

				PlayAnim(Character, Anims.Idle, 0.05, 0.1)
				Idled = true
				Jumped, Falled, Walked, Ran = false, false, false, false
			end
		elseif TorsoVelocity >= runThreshold then
			if not Ran then
				StopAnim(Character, Anims.Jump)
				StopAnim(Character, Anims.Fall)
				StopAnim(Character, Anims.Idle)
				StopAnim(Character, Anims.Walk)

				PlayAnim(Character, Anims.Run, 0.05, 1)
				Ran = true
				Jumped, Falled, Idled, Walked = false, false, false, false
			end
		else
			if not Walked then
				StopAnim(Character, Anims.Jump)
				StopAnim(Character, Anims.Fall)
				StopAnim(Character, Anims.Idle)
				StopAnim(Character, Anims.Run)

				PlayAnim(Character, Anims.Walk, 0.05, 1)
				Walked = true
				Jumped, Falled, Idled, Ran = false, false, false, false
			end
		end
	end
end)


